{
  "name": "headcanon-generator",
  "version": "0.1.0",
  "private": true,
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "ajv": "^6.5.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "firebase": "^5.0.4",
    "prop-types": "^15.6.1",
    "re-base": "^3.2.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "slugify": "^1.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "styles": "postcss ./src/styles/styles.pcss -o ./src/styles/index.css",
    "styles:watch": "postcss ./src/styles/styles.pcss -o ./src/styles/index.css -w",
    "dev": "concurrently --names \"webpack, styles\" --prefix name \"npm run start\" \"npm run styles:watch\""
  },
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "concurrently": "^3.5.1",
    "cssnano": "^3.10.0",
    "postcss-cli": "^5.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-preset-env": "^5.1.0",
    "postcss-syntax": "^0.10.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0"
  }
}
